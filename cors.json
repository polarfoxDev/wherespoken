{
  "CORSRules": [
    {
      "AllowedOrigins": ["http://localhost:4200", "https://wherespoken.games.polarnight.eu"],
      "AllowedMethods": ["GET", "PUT", "POST"],
      "AllowedHeaders": [
        "Authorization",
        "Content-Type",
        "x-amz-date",
        "x-amz-content-sha256",
        "x-amz-user-agent"
      ],
      "ExposeHeaders": ["x-amz-server-side-encryption", "x-amz-request-id", "x-amz-id-2", "ETag"],
      "MaxAgeSeconds": 600
    }
  ]
}
